<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="NotebookTable" parent="GtkBox">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkBox">
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <child>
                    <object class="GtkBox">
                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">dbfx-fpage</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkBox">
                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">dbfx-ppage</property>
                                <signal name="clicked" handler="prePage"/>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="rowNLum">

                    </object>
                </child>
                <child>
                    <object class="GtkBox">
                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">dbfx-npage</property>
                                <signal name="clicked" handler="nextPage"/>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkBox">
                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">dbfx-lpage</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkSeparator">
                        <property name="orientation">vertical</property>
                    </object>
                </child>
                <child>
                    <object class="GtkButton">
                        <property name="icon_name">dbfx-refresh</property>
                        <signal name="clicked" handler="refresh"/>
                    </object>
                </child>
                <style>
                    <class name="table-toolbar"/>
                </style>
            </object>
        </child>
        <child>
            <object class="GtkScrolledWindow" id="scrolledWindow">
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <child>
                    <object class="GtkColumnView" id="tableView">
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="vexpand">True</property>
                        <property name="show_row_separators">True</property>
                        <property name="show_column_separators">True</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
