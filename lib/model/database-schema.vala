/**
 *
 *
 * 数据库Scheme
 *
 * */
public class DatabaseSchema
{
    /**
     *
     *
     * Scheme名称
     *
     * */
    public string name{
        set;
        get;
    }

    /**
     *
     * 字符集
     *
     * */
    public string charset{
        set;
        get;
    }

    /**
     *
     *
     * 字符排序
     *
     * */
    public string collation{
        set;
        get;
    }
}
